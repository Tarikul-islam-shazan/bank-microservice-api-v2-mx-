    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.5.43",
    "@types/morgan": "^1.7.37",
    "@types/node": "^13.5.0",
    "@types/request-promise": "^4.1.45",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.1",
    "@types/uuid": "^3.4.6",
    "@typescript-eslint/eslint-plugin": "^2.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.17.0",

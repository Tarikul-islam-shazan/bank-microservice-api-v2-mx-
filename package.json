{
  "name": "meedbankingclubservice",
  "description": "Meed Banking Club Non-Banking Services",
  "author": "shahwar.maiwandi@gmail.com",
  "keywords": [],
  "license": "ISC",
  "version": "0.0.1",
  "main": "service.js",
  "scripts": {
    "start": "run-s build && cross-env-shell \"NODE_ENV=$NODE_ENV\" nodemon ",
    "stop": "run-s pm2:stop && run-s pm2:delete",
    "dev": "run-s build && cross-env NODE_ENV=development nodemon",
    "info": "npm-scripts-info",
    "build": "run-s clean && run-p build:* && run-s copy:keys",
    "build:main": "tsc -p tsconfig.json",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "eslint": "node node_modules/eslint/bin/eslint src --ext ts",
    "fix:eslint": "npm run eslint -- --fix",
    "test": "run-s fix:* && run-s build test:*",
    "test:lint": "npm run eslint && prettier \"src/**/*.ts\" --list-different",
    "test:unit": "jest \"--coverage=$coverage\"",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "version": "standard-version",
    "reset": "git clean -dfx && git reset --hard && npm i",
    "clean": "trash dist test",
    "prepare-release": "run-s reset test version",
    "copy:keys": "cp -rf ./src/config/keys/ ./dist/config/keys",
    "pm2:dev:start": "cross-env-shell \"NODE_ENV=$NODE_ENV\" && run-s build && pm2 start pm2.yaml",
    "pm2:prod:start": "cross-env-shell \"NODE_ENV=$NODE_ENV\" && pm2 start pm2.yaml",
    "pm2:stop": "pm2 stop pm2.yaml",
    "pm2:delete": "pm2 delete pm2.yaml"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged & npm run fix",
      "pre-push": "npm run build"
    }
  },
  "scripts-old": {
    "postinstall": "npx tsc",
    "dev": "tsc-watch --onSuccess \"node ./dist/server.js\"",
    "start": "npx pm2 start pm2.yaml",
    "test": "jest"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*\\.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/config/"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-extended",
      "reflect-metadata"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/config/**",
      "!src/types/**"
    ],
    "coverageDirectory": "<rootDir>/coverage/"
  },
